<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="697.0" prefWidth="811.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notisblokk.ui.FxAppController">
   <children>
      <TextField fx:id="titleField" layoutX="162.0" layoutY="53.0" prefHeight="38.0" prefWidth="200.0" promptText="title" />
      <HTMLEditor fx:id="messageField" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" layoutX="163.0" layoutY="105.0" prefHeight="446.0" prefWidth="592.0" />
      <Button fx:id="newNote" layoutX="40.0" layoutY="149.0" mnemonicParsing="false" onAction="#onNewNoteClick" prefHeight="27.0" prefWidth="100.0" text="New Note" />
      <Label layoutX="375.0" layoutY="17.0" text="Notepad">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <ListView fx:id="noteListView" layoutX="40.0" layoutY="182.0" onMouseClicked="#onSelectNoteClick" prefHeight="368.0" prefWidth="100.0" />
      <Button fx:id="saveNoteButton" mnemonicParsing="false" onAction="#onSaveClick" style="-fx-background-color: #0080FF;" text="Save" textFill="WHITE" />
      <Button fx:id="deleteNoteButton" mnemonicParsing="false" onAction="#onDeleteClick" style="-fx-background-color: #a6300f;" text="Delete" textFill="WHITE" />
      <TabPane fx:id="categoryTabPane" prefHeight="38.0" prefWidth="697.0" tabClosingPolicy="UNAVAILABLE" />
   </children>
</AnchorPane>
