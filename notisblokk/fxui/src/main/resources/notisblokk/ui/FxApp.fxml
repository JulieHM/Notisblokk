<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.HTMLEditor?>

<AnchorPane prefHeight="697.0" prefWidth="811.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notisblokk.ui.FxAppController">
   <children>
      <HTMLEditor fx:id="messageField" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" layoutX="163.0" layoutY="105.0" prefHeight="446.0" prefWidth="592.0" />
      <Button fx:id="newNote" layoutX="40.0" layoutY="149.0" mnemonicParsing="false" onAction="#onNewNoteClick" prefHeight="27.0" prefWidth="100.0" text="New Note" />
      <ListView fx:id="noteListView" layoutX="40.0" layoutY="182.0" onMouseClicked="#onSelectNoteClick" prefHeight="368.0" prefWidth="100.0" />
      <Button fx:id="saveNoteButton" mnemonicParsing="false" onAction="#onSaveClick" style="-fx-background-color: #0080FF;" text="Save" textFill="WHITE" />
      <Button fx:id="deleteNoteButton" mnemonicParsing="false" onAction="#onDeleteClick" style="-fx-background-color: #a6300f;" text="Delete" textFill="WHITE" />
      <TabPane fx:id="categoryTabPane" prefHeight="38.0" prefWidth="811.0" tabClosingPolicy="UNAVAILABLE" />
      <Button fx:id="newCategory" layoutX="40.0" layoutY="105.0" mnemonicParsing="false" onAction="#onNewCategoryClick" prefHeight="25.0" prefWidth="100.0" text="New Category" />
   </children>
</AnchorPane>
