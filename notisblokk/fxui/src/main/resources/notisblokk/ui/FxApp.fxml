<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.HTMLEditor?>

<AnchorPane prefHeight="484.0" prefWidth="811.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notisblokk.ui.FxAppController">
   <children>
      <HTMLEditor fx:id="messageField" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" layoutX="162.0" layoutY="39.0" prefHeight="445.0" prefWidth="649.0" />
      <Button fx:id="newNote" layoutX="11.0" layoutY="81.0" mnemonicParsing="false" onAction="#onNewNoteClick" prefHeight="27.0" prefWidth="140.0" text="New Note" />
      <!--<Label layoutX="375.0" layoutY="17.0" text="Notepad">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>-->
      <ListView fx:id="noteListView" layoutX="-1.0" layoutY="116.0" onMouseClicked="#onSelectNoteClick" prefHeight="368.0" prefWidth="164.0" />
      <Button fx:id="saveNoteButton" mnemonicParsing="false" onAction="#onSaveClick" style="-fx-background-color: #0080FF;" text="Save" textFill="WHITE" />
      <Button fx:id="deleteNoteButton" mnemonicParsing="false" onAction="#onDeleteClick" style="-fx-background-color: #a6300f;" text="Delete" textFill="WHITE" />
      <TabPane fx:id="categoryTabPane" prefHeight="38.0" prefWidth="811.0" tabClosingPolicy="UNAVAILABLE" />
      <Button fx:id="newCategory" layoutX="11.0" layoutY="45.0" mnemonicParsing="false" onAction="#onNewCategoryClick" prefHeight="27.0" prefWidth="140.0" text="New Category" />
   </children>
</AnchorPane>
